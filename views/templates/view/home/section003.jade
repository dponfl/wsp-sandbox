              .wsp-thumbnail-text {{'HOME_THUMB_01_TEXT' | translate}}

          .col-md-4
            .thumbnail.wsp-thumbnail-no-border
              .wsp-thumbnail
                .item02
                  img(src="../../images/block03-2.png", alt="Camel Car Hire - Section 03-2")
              .wsp-thumbnail-title {{'HOME_THUMB_02_TITLE' | translate}}
              .wsp-thumbnail-text {{'HOME_THUMB_02_TEXT' | translate}}

          .col-md-4
            .thumbnail.wsp-thumbnail-no-border
              .wsp-thumbnail
                .item03
