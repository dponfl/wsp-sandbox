.row
  .container-fluid#know_more
    .parallax-section.section3
      span(data-picture,
      data-alt="Camel Car Hire - Section 03",
      mr-px-parallax,
      mr-px-max-px="0.3").mr-px-img
        span(data-src="../../images/block03.jpg",
        mr-px-size="1920,1078")


      .on-parallax.section3
        .container.text-vertical-center-5
          .col-md-4
            .thumbnail.wsp-thumbnail-no-border
              .wsp-thumbnail
                .item01
                  img(src="../../images/block03-1.png", alt="Camel Car Hire - Section 03-1")
